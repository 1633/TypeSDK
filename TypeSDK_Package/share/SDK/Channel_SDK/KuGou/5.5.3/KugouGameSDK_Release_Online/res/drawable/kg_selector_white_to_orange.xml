<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <corners android:radius="10px" />

            <stroke android:width="1dp" android:color="@color/kg_common_orange" />

            <solid android:color="@color/kg_common_orange" />
        </shape></item>
    <item><shape>
            <corners android:radius="10px" />

            <stroke android:width="1dp" android:color="@color/kg_common_orange" />

            <solid android:color="@color/kg_common_white" />
        </shape></item>

</selector>