<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/lb_user_list_layout"
    android:layout_width="244.0dp"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:background="@drawable/default_lb_select_list_shape"
    android:gravity="center_vertical"
    android:paddingTop="1.0dp"
    android:paddingRight="1.0dp">
    
    <com.liebao.def.sdk.code.component.MyListView
        android:id="@+id/lb_user_list"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:listSelector="@drawable/default_lb_select_list_selector"
        android:divider="#ebebeb"
        android:dividerHeight="0.5dp"
        android:focusable="true"
        android:paddingLeft="0.5dp"
        android:paddingRight="0.5dp"
        android:paddingTop="0.5dp"
        android:paddingBottom="0.5dp"
        android:scrollbarThumbVertical="@drawable/default_lb_scrollbar"/>
    
</LinearLayout>